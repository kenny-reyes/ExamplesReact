    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Attach to Firefox",
            "type": "firefox",
            "request": "attach",
            "reAttach": true,
            "port": 9333,
            "file": "${workspaceFolder}"
        }
    ]
}